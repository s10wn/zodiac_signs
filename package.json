{
  "name": "fqtrend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:unit:report": "vitest --ui --coverage.enabled=true",
    "test:ui": "yarn screenshots-cap && reg-cli __screenshots__/actual __screenshots__/expected __screenshots__/diff -R __screenshots__/report.html -J __screenshots__/reg.json",
    "test:ui:approve": "reg-cli __screenshots__/actual __screenshots__/expected __screenshots__/diff -U -J __screenshots__/reg.json",
    "test:ui:report": "npx http-server __screenshots__ -p 5500 -o /report",
    "test:ui:ci": "reg-cli __screenshots__/actual __screenshots__/expected __screenshots__/diff",
    "code:check": "prettier --check \"src/**/*.{ts,tsx,js,css,scss,html}\"",
    "code:format": "prettier --write \"src/**/*.{ts,tsx,js,css,scss,html}\"",
    "code:format:specific-file": "prettier --write",
    "lint": "eslint --max-warnings 0 .",
    "lint:fix": "eslint --fix .",
    "stylelint:check": "stylelint \"**/*.{css,scss}\"",
    "stylelint:fix": "stylelint \"**/*.{css,scss}\" --fix",
    "update:check": "yarn outdated",
    "update:interactive": "yarn upgrade-interactive",
    "update:all": "yarn yarn-upgrade-all",
    "bundle-analyzer": "npx vite-bundle-visualizer --config ./vite.config.ts",
    "plop": "plop",
    "compose:dev:up": "docker-compose -f deployments/dev/docker-compose.yaml up -d",
    "compose:dev:down": "docker-compose -f deployments/dev/docker-compose.yaml down --volumes --rmi local"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "classnames": "^2.3.2",
    "i18next": "^23.6.0",
    "i18next-browser-languagedetector": "^7.1.0",
    "i18next-http-backend": "^2.3.1",
    "js-cookie": "^3.0.5",
    "react": "^18.2.0",
    "react-cookie": "^7.1.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-i18next": "^13.3.1",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.0",
    "@types/js-cookie": "^3.0.6",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^0.34.4",
    "@vitest/ui": "^0.34.6",
    "browserslist-to-esbuild": "^1.2.0",
    "eslint": "^8.49.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-no-comments": "^1.1.10",
    "eslint-plugin-no-else": "^0.2.2",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^22.1.0",
    "plop": "^4.0.0",
    "postcss": "^8.4.31",
    "postcss-mixins": "^9.0.4",
    "postcss-preset-env": "^9.1.3",
    "postcss-url": "^10.1.3",
    "prettier": "^3.0.3",
    "puppeteer": "^21.6.1",
    "reg-cli": "^0.17.7",
    "storycap": "^4.2.0",
    "stylelint": "^15.10.3",
    "stylelint-config-clean-order": "^5.2.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-prettier": "^4.0.2",
    "typescript": "^5.4.2",
    "typescript-plugin-css-modules": "^5.0.1",
    "vite": "^4.4.9",
    "vite-plugin-svgr": "^4.1.0",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.4",
    "yarn-upgrade-all": "^0.7.2"
  }
}
